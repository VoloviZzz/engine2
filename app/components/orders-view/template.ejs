<div>
	<% if(session.user.admin) { %>
		<% if(order.status == '1') { %>
			<button class="btn">Отправить в работу</button>
		<% } else if(order.status == '2') { %>
			<button class="btn">Завершить</button>
		<% } %>
	<% } %>
</div>
<%= order.id %>
<% order.goods.forEach((goodsItem) => { %>
	<div>
		<%= goodsItem.goods_title -%>
	</div>
<% }) %>