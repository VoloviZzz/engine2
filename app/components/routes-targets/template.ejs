<form class="text-margin-bottom" action="/api/routes-targets/add" method="post">
	<button class="btn">Добавить target_type</button>
</form>
<% routesTargets.forEach(target => { %>
	<div class="text-margin-bottom">
		<% if(user.adminMode) { %>
			<span><%= target.id -%> - </span>
			<input class="padding-10px js-routeTarget-update" data-id="<%= target.id -%>" data-target="title" type="text" value="<%= target.title -%>">
		<% } else { %>
			<span><%= target.title + ' ' + target.id %></span>
		<% } %>
		<% if(user.adminMode) { %>
			<button class="btn bg-delete js-routesTarget-delete" data-id="<%= target.id %>">Удалить</button>
		<% } %>
	</div>
<% }) %>
<% if(user.adminMode) { %>
	<script src="/admins/js/routesTargets.js" defer></script>
<% } %>